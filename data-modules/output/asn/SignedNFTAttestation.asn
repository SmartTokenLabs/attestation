SignedNFTAttestation

DEFINITIONS ::=
BEGIN

IMPORTS
    NFTAttestation
        FROM NFTAttestation
    AlgorithmIdentifier
        FROM AuthenticationFramework;

SignedNFTAttestation ::= SEQUENCE {
    nftAttestation       NFTAttestation,
    signingVersion       INTEGER OPTIONAL,
    signingAlgorithm     AlgorithmIdentifier,
    signatureValue       BIT STRING
}

END
